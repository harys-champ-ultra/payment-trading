
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="stylesheet" href="../css/style.css">
    <link rel="shortcut icon" href="../img/logo.svg" type="image/x-icon">
</head>
<body>
    <header>
        <nav class="web">
            <ul class="left">
                <li><a href="./">Switch to Client</a></li>
            </ul>
            <ul class="center">
                <li><a href="./admin"><img src="./img/logo.svg" alt="PKSuccessTrading"></a></li>
            </ul>
            <ul class="right">
                <% navRight.forEach(element => { %>
                    <li><a href="<%= element.route %>"><%= element.name %></a></li>
                <% }); %>
            </ul>
        </nav>
        <nav class="app">
            <ul class="left">
                <li><a href="./"><img src="./img/logo.svg" alt="PKSuccessTrading"></a></li>
            </ul>
            <ul class="right">
                <li><a onclick="hamburger()"><img src="./img/hbg.svg" alt="Hamburger"></a></li>
            </ul>
        </nav>
        <nav class="mobile">
            <ul>
                <li><a onclick="remover()" href="./">Switch to Client</a></li>
                <% navRight.forEach(element => { %>
                    <li><a onclick="remover()" href="<%= element.route %>"><%= element.name %></a></li>
                <% }); %>
            </ul>
        </nav>
    </header>
