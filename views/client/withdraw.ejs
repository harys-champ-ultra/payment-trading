
    <!-- calling component header -->
    <%- include('./components/header.ejs') -%>
    <main>
        <div class="hero-cont">
            <div class="cont-desc">
                <h1>Withdraw your payment.</h1>
                <p>Please enter payment.</p>
                <form action="./withdrawPost" method="post">
                    <select name="withdrawBank" id="withdrawBank" required>
                        <option selected disabled>Select Withdraw Method</option>
                        <option value="Albarka Bank">Albarka Bank</option>
                        <option value="Alhabib Bank">Alhabib Bank</option>
                        <option value="Allied Bank">Allied Bank</option>
                        <option value="Alflah Bank">Alflah Bank</option>
                        <option value="Dubai Bank">Dubai Bank</option>
                        <option value="Easy Paisa Bank">Easy Paisa Bank</option>
                        <option value="Faisal Bank">Faisal Bank</option>
                        <option value="Habib Bank">Habib Bank</option>
                        <option value="JS Bank">JS Bank</option>
                        <option value="MCB Bank">MCB Bank</option>
                        <option value="Meezan Bank">Meezan Bank</option>
                        <option value="Omni Bank">Omni Bank</option>
                        <option value="Punjab Bank">Punjab Bank</option>
                        <option value="Summit Bank">Summit Bank</option>
                        <option value="Sindh Bank">Sindh Bank</option>
                        <option value="United Bank">United Bank</option>
                    </select>
                    <% withdrawData.forEach(element => { %>
                        <% if((element.total == 0) || (element.total == null)) { %>
                            <% earnedData.forEach(element1 => { %>
                                <% if((element1.total == 0) || (element1.total == null)) { %>
                                    <input type="number" name="withdrawAmount" id="withdrawAmount" minlength="4" max="<%= element1.total %>" value="<%= element1.total %>" placeholder="Enter Amount PKR" disabled required>
                                <% } else { %>
                                    <input type="number" name="withdrawAmount" id="withdrawAmount" minlength="4" max="<%= element1.total %>" value="<%= element1.total %>" placeholder="Enter Amount PKR" required>
                                <% } %>
                            <% }); %>
                        <% } else { %> 
                    <input type="number" name="withdrawAmount" id="withdrawAmount" minlength="4" max="<%= element.total %>" value="<%= element.total %>" placeholder="Enter Amount PKR" required>
                        <% } %> 
                    <% }); %>
                    <input type="text" name="accountTitle" id="accountTitle" placeholder="Title e.g. FIRST LAST" required>
                    <input type="number" name="accountNumber" id="accountNumber" placeholder="Account e.g. 00000000000000" required>
                    <input type="submit" name="clientWithdraw" value="Withdraw">
                </form>
                <p>Looking for? <a href="./deposit">Deposit Payment</a></p>
            </div>
            <figure>
                <img src="./img/deposit.svg" alt="Withdraw Payment">
            </figure>
        </div>
    </main>
